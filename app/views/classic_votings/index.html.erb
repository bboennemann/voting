<p id="notice"><%= notice %></p>

<h1>Listing Classic Votings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @classic_votings.each do |classic_voting| %>
      <tr>
        <td><%= link_to 'Show', classic_voting %></td>
        <td><%= link_to 'Edit', edit_classic_voting_path(classic_voting) %></td>
        <td><%= link_to 'Destroy', classic_voting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Classic voting', new_classic_voting_path %>
