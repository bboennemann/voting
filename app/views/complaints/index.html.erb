<p id="notice"><%= notice %></p>

<h1>Listing Complaints</h1>


<table>
  <tbody>
    <% @complaints.each do |complaint| %>
      <tr>
        <td><%= complaint.id %></td>
        <td><%= complaint.reason %></td>
        <td><%= link_to 'Show', complaint %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
