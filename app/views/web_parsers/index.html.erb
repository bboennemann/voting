<p id="notice"><%= notice %></p>

<h1>Listing Web Parsers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @web_parsers.each do |web_parser| %>
      <tr>
        <td><%= link_to 'Show', web_parser %></td>
        <td><%= link_to 'Edit', edit_web_parser_path(web_parser) %></td>
        <td><%= link_to 'Destroy', web_parser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Web parser', new_web_parser_path %>
